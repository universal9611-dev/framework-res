<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="fill_vertical|center" android:id="@id/background" android:width="@dimen/tw_seekbar_track_height_material">
        <nine-patch android:src="@drawable/sem_progress_bar" android:tint="?^attr-private/semSeekBarNormalColor" />
    </item>
    <item android:gravity="fill_vertical|center" android:id="@id/secondaryProgress" android:width="@dimen/tw_seekbar_track_height_material">
        <scale android:scaleHeight="100%" android:scaleGravity="bottom">
            <nine-patch android:src="@drawable/sem_progress_bar" android:tint="@color/tw_seekbar_color_control_secondary" />
        </scale>
    </item>
    <item android:gravity="fill_vertical|center" android:id="@id/progress" android:width="@dimen/tw_seekbar_track_height_material">
        <scale android:scaleHeight="100%" android:scaleGravity="bottom">
            <nine-patch android:src="@drawable/sem_progress_bar" android:tint="?^attr-private/semSeekBarActivatedColor" />
        </scale>
    </item>
</layer-list>