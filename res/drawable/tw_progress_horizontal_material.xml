<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="fill_horizontal|center" android:id="@id/background" android:height="@dimen/tw_progress_bar_height_material">
        <nine-patch android:src="@drawable/sem_progress_bar" android:tint="?^attr-private/semProgressNormalColor" />
    </item>
    <item android:gravity="fill_horizontal|center" android:id="@id/secondaryProgress" android:height="@dimen/tw_progress_bar_height_material">
        <scale android:scaleWidth="100%">
            <nine-patch android:src="@drawable/sem_progress_bar" android:tint="?^attr-private/semProgressActivatedColor" />
        </scale>
    </item>
    <item android:gravity="fill_horizontal|center" android:id="@id/progress" android:height="@dimen/tw_progress_bar_height_material">
        <scale android:scaleWidth="100%">
            <nine-patch android:src="@drawable/sem_progress_bar" android:tint="?^attr-private/semProgressActivatedColor" />
        </scale>
    </item>
</layer-list>